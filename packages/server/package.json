{
  "name": "hh-orion-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "schema:changes": "yarn build && MIKRO_ORM_CLI_CONFIG=dist/core/mikro-orm.config.js yarn mikro-orm schema:update -d",
    "schema:init": "yarn build && MIKRO_ORM_CLI_CONFIG=dist/core/mikro-orm.config.js yarn mikro-orm schema:update -r",
    "start": "ts-node-dev src/index.ts",
    "test:deps": "depcheck",
    "test:lint": "eslint",
    "test:types": "tsc"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.14.0",
    "@mikro-orm/core": "^6.2.9",
    "@mikro-orm/postgresql": "^6.3.1",
    "cors": "^2.8.5",
    "debug": "^4.3.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "hh-orion-domain": "0.1.0",
    "hh-orion-schema": "0.1.0",
    "jsonwebtoken": "^9.0.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.3.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "src/mikro-orm.config.ts",
      "dist/mikro-orm.config.js"
    ]
  }
}
